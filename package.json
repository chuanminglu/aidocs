{
  "name": "aidocs",
  "version": "1.0.0",
  "description": "AI文档管理系统 - 智能化个人文档管理平台",
  "main": "src/backend/app.py",
  "scripts": {
    "start": "python src/backend/app.py",
    "dev": "python src/backend/app.py --debug",
    "test": "python -m pytest tests/",
    "install": "pip install -r requirements.txt",
    "build": "python setup.py build",
    "clean": "python setup.py clean"
  },
  "keywords": [
    "document-management",
    "ai",
    "knowledge-base",
    "search",
    "template",
    "markdown"
